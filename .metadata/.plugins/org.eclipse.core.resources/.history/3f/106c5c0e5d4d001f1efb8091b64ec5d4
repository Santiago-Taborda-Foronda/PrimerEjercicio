package condicionales;

public class CondicinalDoble {
	public class CondicionalDoble {
	    public static void main(String[] args) {
	        int edad = 16;

	       
	        if (edad >= 18) {
	            System.out.println("Eres mayor de edad.");
	        } else {
	            System.out.println("Eres menor de edad.");
	        }
	    }
	}

}
